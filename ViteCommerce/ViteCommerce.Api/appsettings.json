{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=../container_data/app_database.db;"
  },
  "AllowedHosts": "*",
  "IdentityServer": {
    "Clients": {
      "DotNetReactTemplate": {
        "Profile": "IdentityServerSPA"
      }
    },
    "Key": {
      "Type": "Development",
      "Persisted": true,
      "FilePath": "/app_data/developer_cert_signing.jwk"

    }
  }
}
