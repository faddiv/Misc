{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Docker .NET Core Attach (Preview)",
        "type": "docker",
        "request": "attach",
        "platform": "netCore",
        "containerName": "vitecommerce-api",
        "sourceFileMap": {
          "/src": "${workspaceFolder}"
        }
      },
        {
            "name": "Docker .NET Core Launch",
            "type": "docker",
            "request": "launch",
            "preLaunchTask": "docker-run: debug",
            "netCore": {
                "appProject": "${workspaceFolder}/ViteCommerce.Api/ViteCommerce.Api.csproj"
            }
        }
    ]
}
